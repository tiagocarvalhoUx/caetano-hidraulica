 { "rewrites": [{ "source": "/(.*)", "destination": "/" }], "buildCommand": "npm run vercel-build",
  "installCommand": "npm run vercel-install", "framework": "nextjs","outputDirectory": ".next" ,

 
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ]
}